{
    "name": "incraigulous/adminzone",
    "description": "Administration panel for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "incraigulous",
            "email": "me@craigwan.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "illuminate/support": "^5.7",
        "fzaninotto/faker": "^1.8",
        "jeffochoa/validator-factory": "^1.0",
        "bentools/querystring": "^1.0",
        "spatie/laravel-translatable": "^3.1",
        "spatie/laravel-blade-x": "^2.1",
        "incraigulous/objection": "1.0.1",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "tightenco/ziggy": "^0.6.9",
        "nicolaslopezj/searchable": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Incraigulous\\AdminZone\\": "src/",
            "\\": "database/seeds"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Incraigulous\\AdminZone\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "incraigulous/data-factories": "^1.0",
        "orchestra/testbench": "^3.7",
        "helmich/phpunit-json-assert": "^2.0",
        "rappasoft/laravel-helpers": "^1.0",
        "mockery/mockery": "^1.2",
        "orchestra/testbench-browser-kit": "^3.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Incraigulous\\AdminZone\\AdminZoneServiceProvider"
            ],
            "aliases": {
                "AZ": "Incraigulous\\AdminZone\\AdminZone"
            }
        }
    }
}
